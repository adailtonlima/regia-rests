// relacionamentos

relationship ManyToMany {

    Atividade{dependentes(nome)} to Atividade{precedentes(nome)}    
    Atividade{alocados(nome)} to Pessoa{atividades(nome)}
    
    Aviso{categorias(nome)} to CategoriaEmpresa{avisos(titulo)}  
	Aviso{empresas(nome)} to Empresa{avisos(titulo)}
    Aviso{comunidades(nome)} to Comunidade{avisos(titulo)}
    Aviso{estado(nome)} to Estado{avisos(titulo)}
    Aviso{municipio(nome)} to Municipio{avisos(titulo)}
    Aviso{funcionarios} to Funcionario{avisos(titulo)}
    Aviso{cooperados} to Cooperado{avisos(titulo)}
    
    Banner{categoriasEmpresas(nome)} to CategoriaEmpresa{banners(titulo)}
    
    CategoriaConteudo{categoriasEmpresa(nome)} to CategoriaEmpresa{categoriasConteudos(nome)}
    CategoriaConteudo{conteudos(titulo)} to Conteudo{categorias(nome)}
    
	CategoriaEmpresa{templatesDisponiveis(nome)} to Template{categoriasEmpresa(nome)}
    CategoriaEmpresa{empresas(nome)} to Empresa{categorias(nome)}
    
    CategoriaTemplate{templates(nome)} to Template{categorias(nome)}
    
    ContratoServico{servicos} to Servico{fazParteContratos}
    
    Cooperado{empresas(nome)} to Empresa{cooperados}
    Cooperado{produtos} to Produto{cooperados}
    Cooperado{servicos} to Servico{cooperados}
    
    Empresa{fornecedores(nome)} to Empresa{clientes(nome)}  
    Empresa{impostos(nome)} to Imposto{empresas(nome)}
    
    Projeto{participantes(nome)} to Pessoa{projetos(nome)}
    
    Reuniao{convidados} to Pessoa{convitesReunioes}	// alterado conforme codigo, ates era Cooperado
    
}

relationship OneToOne {

    ContratoServico{notaFiscal(numero)} to NotaFiscal 
	
    Entrada{notaFiscal(numero)} to NotaFiscal
    Entrada{orcamento} to Orcamento // francisco adicionou
    Entrada{orcamento} to ContratoServico // francisco adicionou
    
    MovimentacaoProduto{orcamentoProduto} to OrcamentoProduto // francisco adicionou
    MovimentacaoProduto{compraProduto} to CompraProduto // francisco adicionou
    
    Orcamento {notaFiscal(numero)} to NotaFiscal
    
    Pessoa{user(login)} to User
    
    Reuniao{endereco} to Endereco
    
    Saida{notaFiscal(numero)} to NotaFiscal	
    
}

