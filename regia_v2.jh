relationship OneToMany {

	// francisco adicionou relacionamentos para Atividade
    Atividade{entrada(nome)} to Entrada{atividade(nome)} 
    Atividade{saida(nome)} to Saida{atividade(nome)} 
    Atividade{orcamento} to Orcamento{atividade(nome)} 
    Atividade{arquivos(nome)} to ArquivoAnexo{atividade(nome)}
    Atividade{links(nome)} to LinkAnexo{atividade(nome)}
    Atividade{videos(nome)} to VideoAnexo{atividade(nome)}
    
    Conteudo{ArquivoAnexos(nome)} to ArquivoAnexo{conteudo(titulo)}
    Conteudo{linksAnexos(nome)} to LinkAnexo{conteudo(titulo)}
    Conteudo{videosAnexos(nome)} to VideoAnexo{conteudo(titulo)}
    
    Cooperado{anexos(nome)} to ArquivoAnexo{cooperadoAnexo}
    
    Empresa{empresaCompraProdutos} to CompraProdutos{empresa(nome)}
    Empresa{fornecedorFornecedorProdutos} to CompraProdutos{fornecedor(nome)}
	Empresa{empresaCompraServicos} to CompraServico{empresa(nome)}
	Empresa{fornecedorCompraServicos} to CompraServico{fornecedor(nome)}
	Empresa{contratosCliente} to ContratoServico{cliente(nome)}
    Empresa{contratosFornecedor} to ContratoServico{fornecedor(nome)}
	Empresa{anexos(nome)} to ArquivoAnexo{empresaAnexo(nome)} 
    Empresa{linksAnexos(nome)} to LinkAnexo{empresa(nome)} 
    Empresa{videos(nome)} to VideoAnexo{empresaAnexo(nome)} 
	Empresa{produtosFornecedorPrincipal} to Produto{fornecedorPrincipal}
    Empresa{produtosClientePrincipal} to Produto{clientePrincipal}
	Empresa{entradasEmpresa(nome)} to Entrada{empresa}
    Empresa{entradasOrigem(nome)} to Entrada{origem}
	Empresa{orcamentosFornecedor} to Orcamento{fornecedor(nome)}
    Empresa{orcamentosSolicitante} to Orcamento{solicitante(nome)}
	Empresa{empresas(nome)} to Saida{empresa(nome)}
    Empresa{empresasDestino(nome)} to Saida{destino(nome)}
	Empresa{funcionarios} to Funcionario{empresa(nome)}
    
    Funcionario{anexos(nome)} to ArquivoAnexo{funcionarioAnexo}
    
    Imposto{arquivos(nome)} to ArquivoAnexo{imposto(nome)}
    Imposto{links(nome)} to LinkAnexo{imposto(nome)}
    Imposto{videos(nome)} to VideoAnexo{imposto(nome)}
    
    InstanciaImpostoNaEmpresa{anexos(nome)} to ArquivoAnexo{instanciaImposto}
    InstanciaImpostoNaEmpresa{saidas(nome)} to Saida{instanciaImpostoNaEmpresa} // adicionado francisco
    Patrimonio{arquivos(nome)} to ArquivoAnexo{patrimonio(nome)}
    
    Pessoa{movimentacoesAutorizador} to MovimentacaoProduto{autorizador(nome)}
    Pessoa{movimentacoesResponsavel} to MovimentacaoProduto{responsavel(nome)}
    Pessoa{funcionarios} to Funcionario{pessoa} //cascade em funcion√°rio, antes era OneToOne
    Pessoa{cooperados} to Cooperado{pessoa} // alterado conforme o codigo. Antes era OneToOne
    
    Projeto{entradas(nome)} to Entrada{projeto(nome)}
    Projeto{saidas(nome)} to Saida{projeto(nome)}
    Projeto{atividades(nome)} to Atividade{projeto(nome)}
    
    Template{arquivos(nome)} to ArquivoAnexo{template(nome)}
  	Template{links(nome)} to LinkAnexo{template(nome)}
    Template{videos(nome)} to VideoAnexo{template(nome)}
  
    
   // removido Empresa{servicos(nome)} to Servico{empresa(nome)} nao existe no codigo
    
}

// Set pagination options
paginate * with pagination
dto * with mapstruct
service all with serviceImpl
filter * 

